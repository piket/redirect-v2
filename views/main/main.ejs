  <div class="container">
    <div class="col s12 m12">
      <div class="row header">
        <h3 class="flow-text center">Please enter the url you would like to generate</h3>
      </div>
      <div class="row">
        <form class="col s12" id="gen-url">
          <div class="row">
            <div class="input-field col s12">
              <input id="url" type="url" class="validate">
              <label for="url">URL</label>
            </div>
            <div class="input-field col s12">
              <input id="title" type="text" class="validate">
              <label for="title">title</label>
            </div>
          </div>
          <div class="row center-align">
            <button class="btn waves-effect waves-light gen-btn">Generate URL</button>
          </div>
        </form>
      </div>
      <div class="row">
        <div class="col s12 url-container">
          <h3 class="flow-text center" id="copy-message"></h3>
          <!-- <h4 class="flow-text center click" id="generated-url"></h4> -->
          <input type="text" class="center" id="generated-url" value="">
        </div>
      </div>
    </div>
  </div>

  <script type="text/javascript" src="js/main.js"></script>